{
  "name": "Typed URL History",
  "version": "1.2",
  "description": "Reads your history, groups visited pages by topic and time.",
  "permissions": [
    "history"
  ],
  "browser_action": {
    "default_popup": "typedUrls.html",
    "default_icon": "clock.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2
}